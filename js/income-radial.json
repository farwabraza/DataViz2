{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "background": "#fAf0e6",
  "width": "container",
  "height": 400,
  "data": {
    "values": [
      {"category": "Low Income", "value": 0.184338082},
      {"category": "Lower Middle Income", "value": 0.511963148},
      {"category": "Upper Middle Income", "value": 0.24970276},
      {"category": "High Income", "value": 0.05399601}
    ]
  },
  "layer": [
    {"mark": {"type": "arc", "innerRadius": 30, "stroke": "black"}},
    {
      "mark": {
        "type": "text",
        "radiusOffset": 30,
        "baseline": "middle",
        "dx": 17,
        "dy": -18,
        "fontSize": 12
      },
      "encoding": {
        "text": {"field": "category", "type": "nominal"},
        "color": {"value": "black"}
      }
    }
  ],
  "encoding": {
    "theta": {"field": "value", "type": "quantitative", "stack": true},
    "radius": {
      "field": "value",
      "scale": {"type": "sqrt", "zero": true, "rangeMin": 30}
    },
    "text": {"field": "category", "type": "nominal"},
    "color": {
      "field": "category",
      "type": "nominal",
      "legend": null,
      "scale": {
        "domain": [
          "High Income",
          "Low Income",
          "Upper Middle Income",
          "Lower Middle Income"
        ],
        "range": ["#0571b0", "#ca0020", "#7b3294", "#008837"]
      }
    },
    "tooltip": [
      {"field": "category", "type": "nominal"},
      {
        "field": "value",
        "type": "quantitative",
        "title": "Population Density",
        "format": ".2f"
      }
    ]
  },
  "view": {"stroke": null},
  "config": {}
}
